							return {
								label: item,
								value: item
							};
						}));
					}
				},
				error: function (error) {
					if ((error.status===200)&&(error.readyState=4)) {
						values = JSON.parse(error.responseText);
						response( $.map( values.suggestions, function( item ) {
							return {
								label: item,//__highlight(item, request.term),
			                    value: item
			                    };
						}));
					}
				}
			});
