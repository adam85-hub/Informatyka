Content-Disposition: inline; filename=global-layout.css
Content-Type: text/css; charset=utf-8; name=global-layout.css
Content-Location: http://g1.pcworld.pl/pcwk/_2013/css/global-layout.css?v=20130926b
Content-Transfer-Encoding: 8bit

/* 
defines:
* box-shadow
* bottom
* border
* border-radius
* clear
* display
* left
* list-style
* margin
* overflow
* padding
* position
* right
* top
* z-index
Except on specific HTML text elements (h*, p)
*/
/* outer wrapper for the responsive elements */

.clearb {
	clear:both;
}
body > a {display:none}
#page-wrapper { 
background: none repeat scroll 0 0 #FFFFFF;
width: 100%;
}
#banner { 
left:0;
padding:0 10px;
position:absolute;
top:0;
z-index:52;
}
#home-panels { 
position:relative;
}
.home-panel { 
display:none;
}
.home-panel h2 {
margin:0;
}
/* home page */
.home #logo a.banner-logo { 
margin:8px 0 0 -15px;
}
.home #logo { 
width:410px;
height:50px;
margin-top:20px;
}
.home #logo a.leave-behind { 
display:block;
float:right;
}
/* home hero stuff */
/* on the home page, the main content column is split into two */
.home #main { float:left;max-width:620px; }
#home-main-primary { 
float:right;
width:430px;
}
#home-main-primary h2 { 
border-bottom:1px solid #ccc; 
}
#home-main-primary .excerpt { margin-bottom:25px; overflow:hidden; }
#home-main-primary .excerpt p { margin:0; }
#home-main-primary .excerpt p.post-meta { margin-bottom:5px; }

span.view {
	/*background-image: url(http://g1.pcworld.pl/pcwk/_2013/gfx/popular-icons.png);
	background-repeat;no-repeat;
	width:90px;
	display:block;*/
	font-family:'PT Sans',sans-serif;
	font-size: 12px;	
}

span.low {
	/*background-position: 0 0;*/
	color:#999999;
}
span.normal {
	/*background-position: 0 0;*/
	color: #F37D00;
}
span.warm {
	/*background-position: 0 0;*/
	color: #F15213;
}
span.hot {
	/*background-position: 0 0;*/
	color: #AC1F2B;
}
#home-main-primary .excerpt-img	{ float:left; width:140px; }
#home-main-primary .excerpt-text { float:right; width:280px; }	
#home-main-primary .excerpt a { color:#333; font-family:'PT Sans',sans-serif;font-size:22px; }
body.pcworld #home-main-primary .excerpt a { font-family: "PT Sans",sans-serif; }
#home-main-primary .excerpt a:hover { color:#25A7D7;text-decoration:none;} 
#home-main-primary .excerpt p { color:#333; }
#home-main-primary .excerpt p.post-meta { color:#999; font-size:12px; }
#home-main-primary .excerpt p.post-hed { display:inline;}
#home-main-primary .excerpt p.post-text { font-size:13px; }
#home-main-primary .excerpt p.post-via{ display:inline-block; color:#B5B5B5; text-transform:uppercase; font-size:14px; font-family:'ff-enzo-web',sans-serif; font-weight:bold; }
body.pcworld #home-main-primary .excerpt p.post-via{ font-family: "facitweb",sans-serif; }
body.macworld #home-main-primary .excerpt p.post-via{ font-family: "proxima-nova",sans-serif; }
#home-main-primary .excerpt p.category { font-family:'ff-enzo-web',sans-serif; font-weight:bold; color:#FF8c1e; text-transform:uppercase;}
body.pcworld #home-main-primary .excerpt p.category { font-family: "facitweb",sans-serif; color:#ac1f2b; }
body.macworld #home-main-primary .excerpt p.category { font-family: "proxima-nova",sans-serif; color:#3399cc; }
#home-main-primary .offsite .excerpt-text, #home-main-primary .noImage .excerpt-text, .landing-listing .noImage .excerpt-text { float:none; width:100%; }
.offsite a span, .sistersite a span { background:url(http://g1.pcworld.pl/pcwk/_2013/gfx/arw-offsite.png) 100% 50% no-repeat; padding-right:20px; }
#home-main-primary .listing-footer { border-top:1px solid #e6e6e6; padding-top:10px; }
#home-main-secondary { 
width:160px;float:left;
}
#home-reviews {
background:#c2e8f6;
margin-bottom:20px;
}
#home-reviews h2 { border-bottom:1px solid #fff;margin:0;padding:15px 0; }
#home-reviews h2 span { display:block; }
.home-review { border-bottom:1px solid #fff;border-top:1px solid #bde0ed;padding:15px 10px; }
.home-review .rating { float:none;margin:0 auto; }
.home-review p { margin:0; }
.home-review img { display:block;margin-bottom:5px; }
.home-review .see-all { margin-top:5px; }
#home-featured-brands { border:1px solid #e6e6e6;border-bottom:0; }
